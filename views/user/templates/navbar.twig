{% if login.status == 2 %}
<nav style="width:100%;">
    <div class="blue">
        <div class="nav-wrapper">
          &nbsp;<a href="{{ path_for('user.home') }}" class=""><!-- <i class="fa fa-instagram fa-2x"></i> --><b style="font-size:20px; font-family:initial;font-style:oblique"> Match Making</b></a>
          <ul class="right">
            <li><a href="#modal1" class="modal-trigger" ><i class="material-icons">search</i></a></li>
            <li><a href="{{ path_for('user.statistik') }}" style=""><i class="material-icons">equalizer</i></a></li>
            
            <li><a href="{{ path_for('user.notification') }}"><i class="material-icons">notifications_none</i><!-- <span class="new badge red" style="position:absolute;margin:3px;border-radius:100px;top:2px;right:46px" data-badge-caption="{{notif.count}}"></span> --></a></li>
            <li><a href="{{ path_for('user.my.profil') }}"><i class="material-icons">account_circle</i></a></li>
          </ul>
        </div>
    </div>
</nav>
<br>  

<!-- Modal Structure -->
<div id="modal1" class="modal">
  <div class="modal-content" style="background:#ecf0f1">
    <h5>Search</h5>
     <form class="col s12" action="{{ path_for('user.home') }}" method="GET">
     <div class="input-field col s12 ">
                      <i class="material-icons prefix">search</i>
                      <input id="icon_prefix input_text" type="text" style="border-bottom:1px solid #2c3e50" class="validate" name="search" data-length="20">
                      <label for="icon_prefix" style="color:#2c3e50">Search umur / negara / provinsi / kota </label>
                      </div>
        <div class="modal-footer" style="background:#ecf0f1">
          <button class="btn waves-effect waves-light blue" type="submit">Search</button>
     </form>
        </div>
    </div>
  </div>
</div>
{% else %}
<nav style="width:100%;">
    <div class="blue">
        <div class="nav-wrapper">
          &nbsp;<a href="{{ path_for('user.home') }}" class=""><i class="fa fa-instagram fa-2x"></i><b style="font-size:20px; font-family:initial;font-style:oblique"> Match Making</b></a>
          <ul class="right">
            <li class="disabled" style="cursor: not-allowed"><a href="#modal1" class="modal-trigger" ><i class="material-icons">search</i></a></li>
            <li class="disabled" style="cursor: not-allowed"><a href="{{ path_for('user.statistik') }}" style=""><i class="material-icons">equalizer</i></a></li>
            <li class="disabled" style="cursor: not-allowed"><a href="{{ path_for('user.notification') }}"><i class="material-icons">notifications_none</i><!-- <span class="new badge red" style="position:absolute;margin:3px;border-radius:100px;top:2px;right:46px" data-badge-caption="10"></span> --></a></li>
            <li><a href="{{ path_for('user.my.profil', {id:login.id}) }}"><i class="material-icons">account_circle</i></a></li>
          </ul>
        </div>
    </div>
</nav>
<br>  

<script src="{{ base_url() }}/assets/app-assets/js/core/libraries/jquery.min.js" type="text/javascript"></script>

<script>
  $('body').on('click', '.disabled', function(e) {
    e.preventDefault();
    return false;
});
</script>
{% endif %}
